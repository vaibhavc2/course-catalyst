{
  "name": "@lms/server",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.js",
  "scripts": {
    "build": "rm -rf dist/ && tsc && tsc-alias -p tsconfig.json",
    "build:watch": "pnpm build && (concurrently \"tsc -w --preserveWatchOutput\" \"tsc-alias -w\")",
    "start": "node dist/main.js",
    "build:start": "pnpm build && pnpm start",
    "test:types": "tsc --noEmit",
    "dev": "concurrently \"tsc -w --noEmit --preserveWatchOutput\" \"tsx watch src/main.ts\"",
    "test": "pnpm lint && pnpm test:types",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "tsc": "tsc",
    "prisma:migrate": "pnpm prisma migrate dev",
    "prisma:studio": "pnpm prisma studio",
    "prisma:generate": "pnpm prisma generate",
    "prisma:reset": "pnpm prisma migrate reset --force",
    "prisma:seed": "pnpm prisma db seed --preview-feature",
    "prisma:push": "pnpm prisma db push",
    "prisma:pull": "pnpm prisma db pull",
    "swagger": "node ./docs/swagger.js"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.16.1",
    "argon2": "^0.40.3",
    "axios": "^1.7.2",
    "chalk": "4.1.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "env-var": "^7.5.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^10.0.0",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cookie-parser": "~1.4.7",
    "@types/cors": "~2.8.17",
    "@types/eslint": "~8.56.10",
    "@types/eslint-config-prettier": "~6.11.3",
    "@types/express": "~4.17.21",
    "@types/jsonwebtoken": "~9.0.6",
    "@types/morgan": "~1.9.9",
    "@types/multer": "~1.4.11",
    "@types/node": "^20.14.9",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "~10.0.0",
    "@types/ws": "~8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prisma": "^5.16.1",
    "swagger-autogen": "^2.23.7",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  }
